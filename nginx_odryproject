server {
    listen 80;
    server_name 192.168.0.200;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/pi/odry/program;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/pi/odry/program/odryproject.sock;
    }
}
